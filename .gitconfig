[alias]
    repo-clear = !git rm --cached `git ls-files -i -c --exclude-from=.gitignore`
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true